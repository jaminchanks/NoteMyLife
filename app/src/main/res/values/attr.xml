    <array name="slidingItemIcon">
        <item>@drawable/home</item>
        <item>@drawable/location</item>
        <item>@drawable/water</item>
        <item>@drawable/trend</item>
        <item>@drawable/about</item>
    </array>


    <!-- 顶部日志分类：测试-->
    <string-array name="note_type">
        <item>所有日志</item>
        <item>开发领悟</item>
    </string-array>

