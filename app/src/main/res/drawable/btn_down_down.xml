<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:top="2dp"
        android:right="2dp"
        android:left="2dp"
        android:bottom="2dp"
        >
        <shape android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/light_blue1" />
            <size
                android:width="25dp"
                android:height="25dp" />
        </shape>
    </item>


    <item
        android:top="7dp"
        android:bottom="5dp"
        android:left="11dp"
        android:right="11dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item
        android:left="5dp"
        android:right="5dp"
        android:top="13dp"
        android:bottom="11dp" >
        <shape android:shape="rectangle" >
            <solid android:color="@color/white" />
        </shape>
    </item>

</layer-list>